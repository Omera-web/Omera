{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static@1.0.0" },
    { "src": "api/**/*.js", "use": "@vercel/node@2.64.0" } // Utilise une version sp√©cifique de @vercel/node
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
